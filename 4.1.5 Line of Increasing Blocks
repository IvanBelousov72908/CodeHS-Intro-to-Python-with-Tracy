penup()
backward(150)
pendown()
def repeat_squares():
    for i in range(1):
        forward(10)
        left(90)
        forward(10)
        left(90)
        forward(10)
        left(90)
        forward(10)
        left(90)
        penup()
        forward(20)
        pendown()
repeat_squares()
