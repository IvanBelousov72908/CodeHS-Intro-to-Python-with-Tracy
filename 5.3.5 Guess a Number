secret_number=9
user_number=int(input("Pick a number 1-10"))

def draw_greencheck():
    color("green")
    pensize(10)
    left(45)
    forward(75)
    backward(75)
    left(90)
    forward(30)

while user_number != 9:
    if user_number != 9:
        user_number=int(input("Nope, guess again."))
    elif user_number == 9:
        draw_checkmark()
